<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BaseRecyclerView">
        <attr format="reference" name="bbc_item_layout"/>
    </declare-styleable>

    <declare-styleable name="CurrentTimeView">
        <attr name="ctvFormat" format="string"/>
        <attr name="ctvUpdateInterval" format="integer" />
    </declare-styleable>

    <declare-styleable name="SuperImageView">
        <attr name="sivTransform">
            <enum name="CropTransformation" value="1" />
            <enum name="CropCircleTransformation" value="2" />
            <enum name="CropCircleWithBorderTransformation" value="3" />
            <enum name="CropSquareTransformation" value="4" />
            <enum name="RoundedCornersTransformation" value="5" />
            <enum name="ColorFilterTransformation" value="6" />
            <enum name="GrayscaleTransformation" value="7" />
            <enum name="BlurTransformation" value="8" />
            <enum name="MaskTransformation" value="9" />
        </attr>
    </declare-styleable>
</resources>